<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Submit</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script type="text/javascript" src="/socket.io/socket.io.js"></script>
    <link rel="stylesheet" type="text/css" media="screen" href="/css/bulma.css" />
    <script type = "text/javascript">
        $(document).ready(function (){
            var socket = io();
            socket.on('greeting',function(data){
                console.log(data.msg);
                socket.emit('thankyou',{msg : 'Thank you for connecting me! - Client'});
            });
        });
    </script>
</head>
<body>
    <section class="hero">
        <div class="hero-body">
            <div class="container">
                <p class="title">Dojo Survey Results</p>
            </div>
        </div>
    </section>
    <section class = "section">
        <div class = "container">
            <p id = "emit_top"></p>
            <p id = "emit_bottom"></p>
        </div>
    </section>
    <section class="section">
        <div class="container">
            <div class="levels">
                <div class="level">
                    <div class="level-left">
                        <p>Name: <%= results.name %></p>
                    </div>
                </div>
                <div class="level">
                    <div class="level-left">
                        <p>Location: <%= results.location %></p>
                    </div>
                </div>
                <div class="level">
                    <div class="level-left">
                        <p>Language: <%= results.language %></p>
                    </div>
                </div>
                <div class="level">
                    <p>Comment: </p>
                </div>
                <div class="level">
                    <p><%= results.comment %></p>
                </div>
            </div>
        </div>
    </section>
</body>
</html>